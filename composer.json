{
    "name": "chavary/aspect-mock",
    "description": "Experimental Mocking Framework powered by Aspects",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bodnarchuk",
            "email": "davert@codeception.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "AspectMock": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "walterwoshider/goaop-parser-reflection",
                "version": "1.0.0",

                "source": {
                    "url": "https://github.com/WalterWoshid/goaop-parser-reflection.git",
                    "type": "git",
                    "reference": "php-8"
                },

                "autoload": {
                    "psr-4": {
                        "Go\\ParserReflection\\": "src"
                    },
                    "files": ["src/bootstrap.php"],
                    "exclude-from-classmap": ["/tests/"]
                },

                "require": {
                    "php": "^8.0",
                    "nikic/php-parser": "^4.10.5"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "walterwoshider/goaop-framework",
                "version": "1.0.0",

                "source": {
                    "url": "https://github.com/WalterWoshid/goaop-framework.git",
                    "type": "git",
                    "reference": "php-8"
                },

                "require": {
                    "php": "^8.0",
                    "doctrine/annotations": "^1.11.1",
                    "doctrine/cache": "^1.10",
                    "walterwoshid/dissect": "~1.0",
                    "walterwoshider/goaop-parser-reflection": "^1.0",
                    "laminas/laminas-code": "^4.0",
                    "symfony/finder": "^4.4|^5.1|^6.1"
                },

                "autoload": {
                    "psr-4": {
                        "Go\\": "src/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=7.4",
        "walterwoshider/goaop-framework": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/finder": "^4.4 | ^5.4 | ^6.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/verify": "^2.2",
        "codeception/specify": "^2.0",
        "consolidation/robo": "^3.0"
    }
}
